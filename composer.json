{
    "name": "proximify/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony User Bundle",
    "license": "MIT",
    "keywords": [
        "User management"
    ],
    "authors": [
        {
            "name": "Mert Metin",
            "email": "mert@proximify.ca"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "symfony/http-kernel": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev"
    },
    "autoload": {
        "psr-4": {
            "Proximify\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Proximify\\UserBundle\\Tests\\": "/Tests/"
        }
    }
}